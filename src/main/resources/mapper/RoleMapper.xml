<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jinliang.mapper.RoleMapper">

    <select id="getRole" parameterType="long" resultType="com.jinliang.domain.Role">
        select role_id,role_name,role_note from table_role where role_id =#{roleId}
    </select>

</mapper>